{
  "name": "@bufbuild/cel-box",
  "version": "0.1.0",
  "description": "Embeddable CEL UI",
  "keywords": [
    "javascript",
    "typescript",
    "protobuf",
    "cel",
    "common-expression-language"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bufbuild/cel-es.git",
    "directory": "packages/cel-box"
  },
  "scripts": {
    "gen-icons": "node scripts/gen-icons.js; prettier --write stylesheets/gen/icons.css",
    "gen-stylesheets": "node scripts/gen-stylesheets.js; prettier --write src/gen/stylesheets.ts",
    "prebuild": "rm -rf ./dist/*",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.json --module commonjs --verbatimModuleSyntax false --moduleResolution node10 --outDir ./dist/cjs && echo >./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm": "tsc --project tsconfig.json --outDir ./dist/esm",
    "bundle": "esbuild src/*.ts --bundle --platform=browser --format=esm --tree-shaking=true --outdir=dist/ && prettier dist/* --write",
    "bundle-minified": "esbuild src/*.ts --minify --bundle --platform=browser --format=esm --tree-shaking=true --outdir=dist/",
    "watch": "esbuild src/*.ts --watch --bundle --platform=browser --format=esm --tree-shaking=false --outdir=dist/",
    "format": "biome format --write",
    "lint": "biome lint --error-on-warnings",
    "fix": "biome check --write",
    "fix-unsafe": "biome check --write --unsafe",
    "attw": "attw --pack",
    "license-header": "license-header"
  },
  "type": "module",
  "dependencies": {
    "@bufbuild/cel": "^0.3.0",
    "@bufbuild/cel-spec": "^0.3.0",
    "@types/papaparse": "^5.3.16",
    "papaparse": "^5.5.3",
    "split-grid": "^1.0.11",
    "yaml": "^2.8.1"
  },
  "main": "./dist/cjs/split.js",
  "types": "./dist/cjs/split.d.ts",
  "exports": {
    "./simple": {
      "import": "./dist/esm/simple.js",
      "require": "./dist/cjs/simple.js"
    },
    "./simple/component": {
      "import": "./dist/esm/simple-component.js",
      "require": "./dist/cjs/simple-component.js"
    },
    "./split": {
      "import": "./dist/esm/split.js",
      "require": "./dist/cjs/split.js"
    },
    "./split/component": {
      "import": "./dist/esm/split-component.js",
      "require": "./dist/cjs/split-component.js"
    },
    "./table": {
      "import": "./dist/esm/table.js",
      "require": "./dist/cjs/table.js"
    },
    "./table/component": {
      "import": "./dist/esm/table-component.js",
      "require": "./dist/cjs/table-component.js"
    }
  },
  "typesVersions": {
    "*": {
      "simple": ["dist/cjs/simple.d.ts"],
      "simple/component": ["dist/cjs/simple-component.d.ts"],
      "split": ["dist/cjs/split.d.ts"],
      "split/component": ["dist/cjs/split-component.d.ts"],
      "table": ["dist/cjs/table.d.ts"],
      "table/component": ["dist/cjs/table-component.d.ts"]
    }
  },
  "devDependencies": {
    "esbuild": "^0.25.11",
    "iconoir": "^7.11.0"
  }
}
