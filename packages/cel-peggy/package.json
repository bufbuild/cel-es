{
  "name": "@bufbuild/cel-peggy",
  "version": "0.0.1",
  "description": "A CEL evaluator for ES modules",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bufbuild/cel-es.git",
    "directory": "packages/cel-es"
  },
  "scripts": {
    "generate": "npm run generate:proto && npm run generate:peggy",
    "generate:proto": "buf generate buf.build/alfus/cel:85a8766c403344b78b1f3fd16e4d1f39",
    "generate:peggy": "peggy -o src/grammar.js src/grammar.peggy",
    "test": "npx tsx --test ./src/*.test.ts ./src/*/*.test.ts",
    "postgenerate": "node scripts/peggy2ts.js src/grammar.js",
    "prebuild": "rm -rf ./dist/*",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --project tsconfig.json --module commonjs --verbatimModuleSyntax false --moduleResolution node10 --outDir ./dist/cjs && echo >./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm": "tsc --project tsconfig.json --outDir ./dist/esm",
    "format": "prettier --write --ignore-unknown '.' '!dist' '!src/gen' '!src/pb' '!src/grammar.ts'",
    "lint": "eslint .",
    "attw": "attw --pack"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "@bufbuild/protobuf": "^1.10.0",
    "date-fns-tz": "^2.0.0"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "^1.10.0",
    "peggy": "^4.0.2"
  },
  "files": [
    "dist/**"
  ]
}
