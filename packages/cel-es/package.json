{
  "name": "@bufbuild/cel-es",
  "version": "0.0.1",
  "description": "A CEL evaluator for ES modules",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bufbuild/cel-es.git",
    "directory": "packages/cel-es"
  },
  "scripts": {
    "test": "vitest run",
    "clean": "rm -rf ./dist/*",
    "generate": "peggy -o src/grammar.js src/grammar.peggy",
    "postgenerate": "node scripts/peggy2ts.js src/grammar.js",
    "prebuild": "rm -rf ./dist/*",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "../../node_modules/typescript/bin/tsc --project tsconfig.json --module commonjs --verbatimModuleSyntax false --moduleResolution node10 --outDir ./dist/cjs && echo >./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm": "../../node_modules/typescript/bin/tsc --project tsconfig.json --outDir ./dist/esm"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "dependencies": {
    "@buf/alfus_cel.bufbuild_es": "^1.2.1-20230626203212-40a4ec79694c.1",
    "@bufbuild/protobuf": "^1.2.1",
    "date-fns-tz": "^2.0.0"
  },
  "devDependencies": {
    "vitest": "^0.32.2",
    "peggy": "^4.0.2"
  },
  "files": [
    "dist/**"
  ]
}
