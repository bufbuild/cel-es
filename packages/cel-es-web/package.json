{
  "name": "@bufbuild/cel-es-web",
  "version": "0.0.1",
  "description": "A wasm-based CEL parser for ES modules",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/bufbuild/cel-es.git",
    "directory": "packages/cel-es-web"
  },
  "sideEffects": false,
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "clean": "rm -rf ./dist/cjs/* ./dist/esm/* ./dist/types/*",
    "generate": "",
    "build": "npm run build:cjs && npm run build:esm+types",
    "build:cjs": "tsc --project tsconfig.json --module commonjs --outDir ./dist/cjs && echo >./dist/cjs/package.json '{\"type\":\"commonjs\"}'",
    "build:esm+types": "tsc --project tsconfig.json --module ES2015 --verbatimModuleSyntax --outDir ./dist/esm --declaration --declarationDir ./dist/types && echo >./dist/esm/package.json '{\"type\":\"module\", \"sideEffects\":false}'"
  },
  "dependencies": {
    "@buf/alfus_cel.bufbuild_es": "^1.2.1-20230626203212-40a4ec79694c.1",
    "@bufbuild/cel-es-conformance": "^0.0.1",
    "@bufbuild/cel-es": "^0.0.1",
    "web-tree-sitter": "^0.20.8",
    "jest": "^29.5.0"
  },
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "devDependencies": {
    "esbuild": "^0.18.10",
    "esbuild-jest": "^0.5.0"
  }
}
